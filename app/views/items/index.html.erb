<p id="notice"><%= notice %></p>

<h1>Items</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @items.each do |item| %>
      <tr>
        <td><%= "Y" if item.done %></td>
        <td><%= item.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

